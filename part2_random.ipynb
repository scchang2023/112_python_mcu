    "# 常用亂數套件\n",
    "import random as r\n",
    "str1 = \"abcdefg\"\n",
    "# 從字串中隨機取得一個字元\n",
    "print(r.choice(str1)) # d\n",
    "# 從 n1 到 n2 之間隨機取得一個整數\n",
    "print(r.randint(1, 10)) # 2\n",
    "# 從 0 到 1 之間隨機取得一個浮點數\n",
    "print(r.random()) # 0.2558971477012246\n"
   ]
  }
