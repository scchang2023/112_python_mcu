   "source": [
    "import time as t\n",
    "\n",
    "# 取得目前時間數值\n",
    "print(t.time())\n",
    "\n",
    "# 取得使用者時區的日期及時間資訊\n",
    "print(t.localtime())\n",
    "print(f\"現在是{t.localtime().tm_mon}月\")\n",
    "\n",
    "# 取得使用者時區的日期及時間資訊，並傳回字串格式\n",
    "print(t.ctime())"
   ]
  }
